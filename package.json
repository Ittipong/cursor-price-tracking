{
  "name": "cursor-price-tracking",
  "displayName": "Cursor Price Tracking",
  "description": "VSCode extension for tracking Cursor pricing",
  "version": "0.0.1",
  "icon": "logo.png",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cursorPriceTracking.refresh",
        "title": "Refresh Usage Data",
        "icon": "$(refresh)",
        "category": "Cursor Price Tracking"
      },
      {
        "command": "cursorPriceTracking.configure",
        "title": "Configure Session Token",
        "category": "Cursor Price Tracking"
      },
      {
        "command": "cursorPriceTracking.reset",
        "title": "Reset Configuration",
        "category": "Cursor Price Tracking"
      }
    ],
    "configuration": {
      "title": "Cursor Price Tracking",
      "properties": {
        "cursorPriceTracking.sessionToken": {
          "type": "string",
          "default": "",
          "description": "Session token for authenticating with Cursor's API (WorkosCursorSessionToken cookie value)"
        }
      }
    },
    "viewsContainers": {
      "panel": [
        {
          "id": "cursorPricePanel",
          "title": "Cursor Price Tracking",
          "icon": "$(dollar)"
        }
      ]
    },
    "views": {
      "cursorPricePanel": [
        {
          "id": "cursorPrices",
          "name": "Cursor Usage",
          "when": "true"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "cursorPriceTracking.refresh",
          "when": "view == cursorPrices",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/node": "^22.0.0",
    "typescript": "^5.7.0"
  }
}